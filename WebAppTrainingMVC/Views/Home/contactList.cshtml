
@{
    ViewData["Title"] = "contactList";
}

<h1>contactList</h1>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Message</th>
            
            </tr>

    </thead>

    <tbody>

   
    @if (ViewBag.contactList !=null && ViewBag .contactList is List<string[]>)
    {
        foreach (string[]  item in ViewBag.contactList)
                {
                    <tr>
                        <td>@item[0]</td>
                        <td>@item[1]</td>
                        <td>@item[2]</td>
                    </tr>
                }
    }
    </tbody> 
</table>

